<response>
	<date _mapper="uk.emarte.reverse.velocity.mapping.DateMapper[dd-MM-yyyy]">$response-timestamp</date>
	<message>$response-message</message>
	<product type="$response-product-types">
		<id _mapper="uk.emarte.reverse.velocity.mapping.JavaTypesMapper[INT]">$response-product-ids</id>
		<name>$response-product-names</name>
		<description>$response-product-descriptions</description>
	</product>
</response>