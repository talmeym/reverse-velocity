<rv:response _insert="response" _class="uk.emarte.reverse.velocity.test.Response" xmlns:rv="http://emarte.uk/reverse-velocity">
	<rv:date _mapper="uk.emarte.reverse.velocity.mapping.DateMapper[dd-MM-yyyy]">$response.timestamp</rv:date>
	<rv:message>$response.message</rv:message>
	<rv:product _insert="response.products" _class="uk.emarte.reverse.velocity.test.Product" type="$response.products.type" _mapper="@type:uk.emarte.reverse.velocity.test.ProductTypeMapper">
		<rv:id>$response.products.id</rv:id>
		<rv:name>$response.products.name</rv:name>
		<rv:description>$response.products.description</rv:description>
	</rv:product>
</rv:response>