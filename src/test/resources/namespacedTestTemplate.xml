<rv:response insert="response" class="com.emarte.reverse.velocity.test.Response" xmlns:rv="http://www.emarte.org/reverse-velocity">
	<rv:date mapper="com.emarte.reverse.velocity.mapping.DateMapper[dd-MM-yyyy]">$response.timestamp</rv:date>
	<rv:message>$response.message</rv:message>
	<rv:product insert="response.products" class="com.emarte.reverse.velocity.test.Product" type="$response.products.type" mapper="@type:com.emarte.reverse.velocity.test.ProductTypeMapper">
		<rv:id>$response.products.id</rv:id>
		<rv:name>$response.products.name</rv:name>
		<rv:description>$response.products.description</rv:description>
	</rv:product>
</rv:response>